  "type": "object",
  "additionalProperties": false,
  "properties": {
    "nino": {
      "$ref": "NinoWithoutSuffix.json"
    },
    "individualDetails" : {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "ninoSuffix": {
          "type": "string"
        },
        "sex": {
          "type": "string"
        },
        "adultRegSerialNumber": {
          "type": "string"
        },
        "dateOfBirth": {
          "$ref": "IsoDate.json"
        },
        "dateOfDeath": {
          "$ref": "IsoDate.json"
        },
        "dateOfEntry": {
          "$ref": "IsoDate.json"
        },
        "dateOfRegistration": {
          "$ref": "IsoDate.json"
        },
        "mergeStatus": {
          "type": "integer"
        },
        "marriageStatusType": {
          "type": "integer"
        },
        "ntTaxCodeInd": {
          "type": "integer"
        },
        "liveCapacitorInd": {
          "type": "integer"
        },
        "accountStatusType": {
          "type": "integer"
        },
        "dateOfDeathStatus": {
          "type": "integer"
        },
        "permanentTSuffixCaseIndicator": {
          "type": "integer"
        },
        "crnIndicator": {
          "type": "integer"
        },
        "dateOfBirthStatus": {
          "type": "integer"
        },
        "registrationType": {
          "type": "integer"
        }
      }
    }
  },
  "required": ["nino"]
}
